{
  "name": "website",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "develop": "yarn run start",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "deploy": "docusaurus deploy",
    "api-docs": "yarn api-docs:markdown & yarn api-docs:json",
    "api-docs:json": "typedoc --json src/api.json --theme ./typedoc/website",
    "api-docs:markdown": "typedoc --theme ./typedoc/website --hideBreadcrumbs",
    "api-docs:html": "typedoc --plugin none --theme ./typedoc/html --readme ../packages/react-day-picker/README.md"
  },
  "dependencies": {
    "@docusaurus/core": ">=2.0.0-alpha.50",
    "@docusaurus/preset-classic": ">=2.0.0-alpha.50",
    "buble": "^0.20.0",
    "classnames": "^2.2.6",
    "date-fns": "^2.12.0",
    "mdx-loader": "^3.0.2",
    "react": "^16.13.1",
    "react-day-picker": ">=8.0.0-alpha.42",
    "react-dom": "^16.13.1",
    "react-frame-component": "^4.1.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "prop-types": "^15.7.2",
    "raw-loader": "^4.0.1",
    "react-docgen": "^5.3.0",
    "typedoc": "^0.17.4",
    "typedoc-plugin-markdown": "^2.2.17",
    "webpack": "^4.42.1"
  }
}
