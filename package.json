{
  "name": "react-day-picker",
  "version": "0.8.3",
  "description": "Minimalistic date picker component for React and momentjs.",
  "main": "./lib/DayPicker.js",
  "scripts": {
    "prepublish": "babel ./src -d ./lib",
    "coverage": "istanbul cover --report html --report text -- _mocha tests/* --compilers js:babel/register --recursive --reporter xunit",
    "test": "mocha tests/* --compilers js:babel/register --recursive --reporter spec"
    "test:watch": "mocha tests/* -w --compilers js:babel/register --recursive --reporter spec"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gpbl/react-day-picker.git"
  },
  "keywords": [
    "react",
    "react-component",
    "component",
    "calendar",
    "date-picker",
    "datepicker",
    "date",
    "picker",
    "moment",
    "momentjs"
  ],
  "author": "Giampaolo Bellavite <io@gpbl.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gpbl/react-day-picker/issues"
  },
  "homepage": "https://github.com/gpbl/react-day-picker",
  "peerDependencies": {
    "moment": "^2.9.0",
    "react": "~0.13.x"
  },
  "devDependencies": {
    "babel": "^5.4.7",
    "babel-core": "^5.4.7",
    "istanbul": "^0.3.14",
    "jsdom": "^3.1.2",
    "moment": "^2.10.3",
    "react": "^0.13.3",
    "react-tap-event-plugin": "^0.1.7",
    "sinon": "^1.14.1",
    "sinon-chai": "^2.7.0"
  }
}
