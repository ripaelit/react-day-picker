{
  "sortConfiguration": {
    "defaultOrderLevel": 30,
    "customOrderingRules": {
      "rules": [
        {
          "orderLevel": 1,
          "regex": "react"
        },
        {
          "orderLevel": 40,
          "regex": "^(contexts|components|hooks|types|test)$"
        },
        {
          "orderLevel": 50,
          "regex": "^[.]"
        }
      ]
    }
  }
}
